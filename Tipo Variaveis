public class TipoVariaveis{
    public static void main(String[] args){

        int valor = 10;
        System.out.println( "O valor é: " + valor);
        
        int soma = 5 + 5;
        System.out.println( "A soma é: " + soma);
        
      int idade = 20;
      System.out.println("A idade é: " + idade * 2);



    }

}
